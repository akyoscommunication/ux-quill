{
  "name": "@akyoscommunication/ux-quill",
  "description": "File input rich editor text for Symfony Forms",
  "license": "MIT",
  "version": "1.0.0",
  "main": "dist/main.js",
  "config": {
    "css_source": "src/app.css"
  },
  "symfony": {
    "controllers": {
      "ux-quill": {
        "main": "dist/controllers/ux-quill_controller.js",
        "name": "ux-quill",
        "webpackMode": "eager",
        "fetch": "eager",
        "enabled": true,
        "autoimport": {
          "@akyoscommunication/ux-quill/dist/app.css": true
        }
      }
    },
    "importmap": {
      "@hotwired/stimulus": "^3.0.0",
      "@akyoscommunication/main": "path:%PACKAGE%/dist/main.js"
    }
  },
  "peerDependencies": {
    "@hotwired/stimulus": "^3.0.0"
  },
  "devDependencies": {
    "@hotwired/stimulus": "^3.0.0"
  },
  "dependencies": {
    "quill": "^2.0.3"
  }
}
